                'regDate': None,
                'address': None,
                'workTime': None,
                'telephone': None,
                'fax': None,
                'email': None
            },
            '/sveden/education': {
                    'eduOp': {
                        'eduCode': None,
                        'eduName': None,
                        'eduLevel': None,
                        'eduForm': None,
                        'opMain': None,
                        'educationPlan': None,
