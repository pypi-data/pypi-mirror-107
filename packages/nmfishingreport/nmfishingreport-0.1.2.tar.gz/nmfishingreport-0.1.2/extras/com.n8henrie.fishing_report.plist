<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.n8henrie.fishing_report</string>
        <key>ProgramArguments</key>
        <array>
            <string>/path/to/nmfishingreport/venv/bin/python3</string>
            <string>-m</string>
            <string>nmfishingreport</string>
            <string>-c</string>
            <string>/path/to/nmfishingreport/config.ini</string>
        </array>
        <key>StartCalendarInterval</key>
        <dict>
            <key>Hour</key>
            <integer>02</integer>
            <key>Minute</key>
            <integer>00</integer>
        </dict>
        <key>StandardErrorPath</key>
        <string>/path/to/nmfishingreport/error.log</string>
        <key>StandardOutPath</key>
        <string>/dev/null</string>
    </dict>
</plist>
