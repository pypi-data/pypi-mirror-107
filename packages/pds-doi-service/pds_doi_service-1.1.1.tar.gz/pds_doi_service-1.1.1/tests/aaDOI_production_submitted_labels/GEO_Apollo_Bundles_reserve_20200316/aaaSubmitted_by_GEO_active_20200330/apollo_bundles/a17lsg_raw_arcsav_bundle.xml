    <title>Apollo 17 ALSEP ARCSAV Lunar Surface Gravimeter Raw Cleaned ASCII Data Bundle</title>
    <information_model_version>1.11.0.0</information_model_version>
    <product_class>Product_Bundle</product_class>
    <Citation_Information>
      <author_list>Nakamura, Y.; Nagihara, S.</author_list>
